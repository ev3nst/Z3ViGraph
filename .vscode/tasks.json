{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/ViGraph/ViGraph.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/ViGraph/ViGraph.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/ViGraph/ViGraph.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary",
                "--project",
                "${workspaceFolder}/ViGraph/ViGraph.csproj",
                "-s",
                "${workspaceFolder}/ViGraph/ViGraph.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "dbrollback",
            "command": "dotnet",
            "type": "process",
            "args": [
                "ef",
                "migrations",
                "remove",
                "--project",
                "${workspaceFolder}/ViGraph.Database/ViGraph.Database.csproj",
                "-s",
                "${workspaceFolder}/ViGraph/ViGraph.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "dbupdate",
            "command": "dotnet",
            "type": "process",
            "args": [
                "ef",
                "database",
                "update",
                "-s",
                "${workspaceFolder}/ViGraph/ViGraph.csproj"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}